<table class='search searchresults'>
<tr><td>torrent</td><td>magnet</td></tr>
<% if 0 && @torrents.length > 0 %>
<% @torrents.each {|torrent| %>
<tr>
<td><a href='<%= "#{@upload_dir}/#{torrent[:url]}" %>'><%= torrent[:name] %></a></td>
<td><a href='<%= torrent[:magnet] %>'>magnet</a></td>
</tr>
<% } %>
<% else %>
<tr><td>No torrents match.</td></tr>
<% end %>
</table>
